{
  "name": "@equt/xlsx",
  "version": "0.1.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "unbuild",
    "check": "tsc",
    "format": "prettier -c src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@equt/eslint-config": "^8.9.0",
    "@equt/prelude": "^0.1.12",
    "@equt/prettier-config": "^2.5.1",
    "@equt/tsconfig": "^1.3.1",
    "@types/jest": "^29.4.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1"
  },
  "unbuild": {
    "entries": [
      "src/index"
    ],
    "clean": true,
    "declaration": true,
    "rollup": {
      "emitCJS": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@equt/eslint-config"
    ]
  },
  "prettier": "@equt/prettier-config",
  "jest": {
    "preset": "ts-jest/presets/default-esm"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "@equt/prelude": "^0.1.6"
  }
}
